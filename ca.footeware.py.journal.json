{
    "id" : "ca.footeware.py.journal",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "command" : "journal",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
    {
      "name": "python3-jprops",
      "buildsystem": "simple",
      "build-commands": [
        "pip3 install --no-deps --prefix=/app ."
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://github.com/mgood/jprops/archive/refs/tags/2.0.2.zip",
          "strip-components": 1,
          "sha256": "b555a427655aedb22ba7e1a9dd20103ea5fa2c87ba21f5c7b72e074a6fd86be3"
        }
      ]
    },
    {
    		"name" : "journal",
			"builddir" : true,
			"buildsystem" : "meson",
			"sources" : [
				{
					"type" : "git",
					"url" : "file:///home/craig/git"
				}
      ]
    }
  ]
}
